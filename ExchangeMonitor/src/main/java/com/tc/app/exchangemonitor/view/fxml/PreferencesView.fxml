<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="15.0" xmlns="http://javafx.com/javafx/8.0.65" 	xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.tc.app.exchangemonitor.controller.PreferencesController">
<children>
		<CheckBox fx:id="resetCredentialsCheckBox" onAction="#handleResetCredentialsCheckBoxClick" text="Reset Credentials" />
		<CheckBox fx:id="displayAccountsWithPermissionCheckBox" onAction="#handleDisplayAccountsWithPermissionCheckBoxClick" text="Display Accounts With Permission" />
		<CheckBox fx:id="enableAnimationsCheckBox" onAction="#handleEnableAnimationsCheckBoxClick" text="Enable Animations" />
		<CheckBox fx:id="enableRowColorsCheckBox" onAction="#handleEnableRowColorsCheckBoxClick" text="Enable Row Colors" />
		<CheckBox fx:id="enableRowToolTipsCheckBox" onAction="#handleEnableRowToolTipsCheckBoxClick" text="Enable Row ToolTips" />
		<CheckBox fx:id="enableRowContextMenuCheckBox" onAction="#handleEnableRowContextMenuCheckBoxClick" text="Enable Row Context Menu" />
</children>
<padding><Insets bottom="10.0" left="10.0" right="10.0" top="10.0" /></padding>
</VBox>